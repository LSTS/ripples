{
    "extends": [
      "tslint:recommended",
      "tslint-react"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "interface-name": false,
      "jsx-no-lambda": false,
      "no-var-requires": false,
      "forin": false,
      "max-classes-per-file": false,
      "object-literal-sort-keys": false
    }
  }